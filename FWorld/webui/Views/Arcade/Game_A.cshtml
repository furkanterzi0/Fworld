@{
    Layout=null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="_head">
    <style>
        #gamePlace {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);      
        }
        #restart {
            position: absolute;
            top: 60%;
            left: 50%;
            transform: translate(-50%, -50%);      
        }
    </style>
</head>

<body style="background-color: rgb(58, 55, 56);">
    
    <header>
        <partial name="_nav">
    </header>

    <div id="gamePlace">
        <button id="strButton"> Start </button>
        
    </div>
    <div id="restart"> </div>
    <script src="~/js/GameA.js"></script>
    <script src="~/js/themeMode.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

</body>
</html>
